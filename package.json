{
  "name": "i18next-redux-saga",
  "version": "2.0.0-alpha",
  "description": "i18next binding to redux using saga",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "build": "webpack -p",
    "build:dev": "webpack",
    "start": "webpack --watch",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:prod": "yarn lint && yarn test --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexiusp/i18next-redux-saga.git"
  },
  "keywords": [
    "i18next",
    "redux",
    "saga"
  ],
  "author": "Aleksei Podgaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexiusp/i18next-redux-saga/issues"
  },
  "homepage": "https://github.com/alexiusp/i18next-redux-saga#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^10.15.0",
    "@types/webpack": "^3.8.8",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-loader": "^8.1.0",
    "eslint": "^7.21.0",
    "i18next": "^19.9.0",
    "jest": "^26.6.3",
    "redux": "^4.0.0",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "^4.0.1",
    "regenerator-runtime": "^0.13.7",
    "source-map-loader": "^1.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!<rootDir>/node_modules/",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 60,
        "functions": 78,
        "lines": 80
      }
    }
  }
}
